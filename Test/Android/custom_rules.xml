<?xml version="1.0" encoding="UTF-8"?>
<project name="Test" default="help">
    <target name="-pre-build">
        <exec executable="${ndk.dir}/ndk-build" failonerror="true">
        	<arg value="NDK_DEBUG=1"/>
        </exec>
    </target>

    <target name="clean" depends="android_rules.clean">
        <exec executable="${ndk.dir}/ndk-build" failonerror="true">
            <arg value="clean"/>
        </exec>
    </target>
</project>
